<simulation>
  <control>
    <duration>{{ defaults.get('simdur') }}</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
    <decay>{{ defaults.get('decay') }}</decay>
    <explicit_inventory>{{ defaults.get('expinv') }}</explicit_inventory>
    <explicit_inventory_compact>{{ defaults.get('expinv_compact') }}</explicit_inventory_compact>
  </control>

  <archetypes>
    <spec> <lib>agents</lib> <name>NullRegion</name> </spec>
    <spec> <lib>agents</lib> <name>NullInst</name> </spec>
    <spec> <lib>cycamore</lib> <name>DeployInst</name> </spec>
    <spec> <lib>cycamore</lib> <name>Source</name> </spec>
    <spec> <lib>cycamore</lib> <name>Sink</name> </spec>
    <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
  </archetypes>

  <facility>
    <name>fuel_source</name>
    <config>
      <Source>
        <outcommod>fresh_fuel</outcommod>
        <outrecipe>fresh_fuel</outrecipe>
        <throughput>1e100</throughput>
      </Source>
    </config>
  </facility>


  <facility>
    <name>fuel_sink</name>
    <config>
      <Sink>
        <in_commods><val>spent_fuel</val></in_commods>
        <capacity>1e100</capacity>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>lw_reactor</name>
    <!-- <lifetime>960</lifetime> -->
    <config>
      <Reactor>
        <fuel_inrecipes>  <val>fresh_fuel</val> </fuel_inrecipes>
        <fuel_outrecipes> 
          <val>{{ defaults.get('outrecipe') }}</val>
        </fuel_outrecipes>
        <fuel_incommods>  <val>fresh_fuel</val> </fuel_incommods>
        <fuel_outcommods> <val>spent_fuel</val> </fuel_outcommods>
        <cycle_time>{{ defaults.get('cycletime') }}</cycle_time>
        <refuel_time>0</refuel_time>
        <assem_size>20000</assem_size>
        <n_assem_core>3</n_assem_core>
        <n_assem_batch>1</n_assem_batch>
        <power_cap>1000</power_cap>
      </Reactor>
    </config>
  </facility>


  <region>
    <name>NullRegion</name>
    <config><NullRegion/></config>
    <institution>
      <name>DeployInst</name>
        <initialfacilitylist>
          <entry>
            <prototype>fuel_source</prototype>
            <number>1</number>
          </entry>
          <entry>
            <prototype>fuel_sink</prototype>
            <number>1</number>
          </entry>
          <entry>
            <prototype>lw_reactor</prototype>
            <number>{{ defaults.get('facnum') }}</number>
          </entry>
        </initialfacilitylist>
        <config><NullInst/></config>
<!--        <config>
          <DeployInst>
            <prototypes>
              <val>lw_reactor</val>
            </prototypes>
            <build_times>
              <val></val>
              <val></val>
              <val></val>
            </build_times>
            <n_build>
              <val></val>
              <val></val>
              <val></val>
            </n_build>
	      </DeployInst>
        </config> -->
    </institution>
  </region>

